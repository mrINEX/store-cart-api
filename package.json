{
  "name": "rs-checkout-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sls:offline": "nest build & npx serverless offline",
    "orm:run": "nest build & node dist/rds.service.js",
    "ts:node": "node node_modules/.bin/ts-node src/rds.service.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "latest",
    "@nestjs/core": "latest",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/typeorm": "^9.0.1",
    "@vendia/serverless-express": "^4.10.3",
    "aws-lambda": "latest",
    "dotenv": "^16.0.3",
    "helmet": "latest",
    "passport": "latest",
    "passport-http": "latest",
    "passport-jwt": "^4.0.1",
    "passport-local": "latest",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.16",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^5.0.4",
    "uuid": "latest"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "latest",
    "@nestjs/testing": "^9.4.0",
    "@types/aws-lambda": "latest",
    "@types/express": "latest",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/jest": "latest",
    "@types/node": "^20.1.3",
    "@types/passport-jwt": "latest",
    "@types/passport-local": "latest",
    "@types/supertest": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-import": "latest",
    "jest": "latest",
    "prettier": "latest",
    "rimraf": "latest",
    "serverless-offline": "latest",
    "supertest": "latest",
    "ts-jest": "latest",
    "ts-loader": "latest",
    "ts-node": "latest",
    "tsconfig-paths": "latest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
